@model Thunder.ApplicationCore.Entities.TbChannel
@{
    ViewBag.Title = "Order";
    Layout = "~/Views/Shared/_PageLayout.cshtml";
}
<body>
<section class="content-header">
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index","Home")"><i class="fa fa-dashboard"></i>首页</a></li>
        <li>@Html.ActionLink("财务管理", "Index", "Finance")</li>
        <li>@Html.ActionLink("汇率管理", "Index", "Channel")</li>
        <li class="active">新增汇率</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="jax-box">
        <form id="siteForm" class="form-horizontal">
            <div class="box-body">
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">币种</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="Title" id="add_Title" />
                    </div>
                </div>
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">汇率</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="Title" id="add_Title" />
                    </div>
                </div>
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">生效时间</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="paId" id="add_PaId" />
                    </div>
                </div>
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">失效时间</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="ExtendId" id="add_ExtendId" />
                    </div>
                </div>                
                <div class="form-group col-sm-11">
                    <div class="col-lg-4 col-sm-5"></div>
                    <div class="col-lg-4 col-sm-4">
                        <button type="button" class="btn btn-primary" id="add_saveBtn">保存</button>
                    </div>
                </div>
            </div>
            
        </form>
    </div>
</section>
</body>