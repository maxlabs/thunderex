@model Thunder.ApplicationCore.Entities.TbChannel
@{
    ViewBag.Title = "Order";
    Layout = "~/Views/Shared/_PageLayout.cshtml";
}
<body>
<section class="content-header">
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index","Home")"><i class="fa fa-dashboard"></i>首页</a></li>
        <li><a href="#">国内客服</a></li>
        <li><a href="#">申报材料</a></li>
        <li class="active">上传申报材料</li>
    </ol>
</section>
<!-- Main content -->
<section class="content">
    <div class="jax-box">
        <form id="siteForm" class="form-horizontal">
            <div class="box-body">
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">寄件方</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="Title" id="add_Title" />
                    </div>
                </div>
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">寄件方证件号</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="Title" id="add_Title" />
                    </div>
                </div>
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">寄件方证件</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="paId" id="add_PaId" />
                    </div>
                    <div class="col-lg-2 col-sm-1">
                        <button type="button" class="btn btn-primary" id="add_saveBtn">上传</button>
                    </div>
                </div>
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">收件方</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="Title" id="add_Title" />
                    </div>
                </div>
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">收件方证件号</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="Title" id="add_Title" />
                    </div>
                </div>
                <div class="form-group col-sm-11">
                    <label class="col-lg-2 col-sm-3 control-label">收件方证件</label>
                    <div class="col-lg-6 col-sm-8">
                        <input type="text" class="form-control" name="paId" id="add_PaId" />
                    </div>
                    <div class="col-lg-2 col-sm-1">
                        <button type="button" class="btn btn-primary" id="add_saveBtn">上传</button>
                    </div>
                </div>               
                <div class="form-group col-sm-11">
                    <div class="col-lg-4 col-sm-5"></div>
                    <div class="col-lg-4 col-sm-4">
                        <button type="button" class="btn btn-primary" id="add_saveBtn">保存</button>
                    </div>
                </div>
            </div>
            
        </form>
    </div>
</section>
</body>